{
    "name": "aurorawebsoftware/connective",
    "description": "This is my package connective",
    "keywords": [
        "AuroraWebSoftware",
        "laravel",
        "connective"
    ],
    "homepage": "https://github.com/aurorawebsoftware/connective",
    "license": "MIT",
    "authors": [
        {
            "name": "AuroraWebSoftwareTeam",
            "email": "websoftwareteam@aurorabilisim.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2|^8.3",
        "spatie/laravel-package-tools": "^1.14.0",
        "laravel/framework": "^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^8.0",
        "nunomaduro/larastan": "^3",
        "orchestra/testbench": "^9",
        "pestphp/pest": "^3",
        "pestphp/pest-plugin-arch": "^3",
        "pestphp/pest-plugin-laravel": "^3",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "AuroraWebSoftware\\Connective\\": "src/",
            "AuroraWebSoftware\\Connective\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AuroraWebSoftware\\Connective\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-connective --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AuroraWebSoftware\\Connective\\ConnectiveServiceProvider"
            ],
            "aliases": {
                "Connective": "AuroraWebSoftware\\Connective\\Facades\\Connective"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
